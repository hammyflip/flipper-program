{
  "compilerOptions": {
    "baseUrl": "src",
    "lib": ["es2015"],
    "module": "commonjs",
    "target": "es6",
    "strict": true,
    "noEmit": true,
    "skipLibCheck": true,
    "esModuleInterop": true,
    "types": ["jest"],
    "typeRoots": ["./node_modules/@types"],
  },
  "include": [
    "src",
  ],
  "exclude": [
    "node_modules"
  ]
}
